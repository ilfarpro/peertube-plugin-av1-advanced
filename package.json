{
    "name": "peertube-plugin-av1-advanced",
    "version": "0.4.7",
    "license": "MIT",
    "description": "Adds SVT-AV1 transcoding profile. Uses 8-bit, fast-decode, tiling for compatibility. Available settings: max bitrate per resolution, SVT-AV1 presets, CRF. Use it with latest FFMPEG to get best from SVT-AV1 encoder, check plugin repo for info.",
    "engine": {
        "peertube": ">=5.1.0"
    },
    "keywords": [
        "peertube",
        "plugin",
        "av1",
        "trascode",
        "encode"
    ],
    "homepage": "https://github.com/ilfarpro/peertube-plugin-av1-advanced",
    "author": "gdsn",
    "bugs": "https://github.com/ilfarpro/peertube-plugin-av1-advanced/issues",
    "library": "./dist/main.js",
    "files": [
        "/dist"
    ],
    "staticDirs": {},
    "css": [],
    "clientScripts": [],
    "translations": {},
    "scripts": {
        "clean": "rm -rf dist/*",
        "prepare": "npm run build",
        "build": "npx tsc --build src/tsconfig.json",
        "watch": "npx tsc --build src/tsconfig.json --watch"
    },
    "devDependencies": {
        "@peertube/peertube-types": "^5.1.0",
        "@tsconfig/node16": "^1.0.3",
        "typescript": "^5.0.4"
    }
}
